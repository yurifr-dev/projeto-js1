
// Definindo as variáveis
var a = 10;
var b = 5;
var c = 1;
var d;

// Realizando as operações e imprimindo os valores
c = b--;
document.write(`<span>Valor de c após c = b--: ${c}</span><br/> `);
b = a++;
document.write(`<span>Valor de b após b = a++: ${b}</span><br/> `);

a = 5 + "1";
document.write(`<span>Valor de a após a = 5 + "1": ${a}</span><br/> `);

d = "1" + "1";
document.write(`<span>Valor de d após d = "1" + "1": ${d}</span><br/> `);
